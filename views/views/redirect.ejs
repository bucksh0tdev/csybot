<!DOCTYPE html>
<html lang="en">
<head>
<%- header %>
</head>
<body>
<div class="select-project">
    <center>
       <h2 id="msg" style="color: #ad91ff;"><%- msg %> (5)</h2>
    </center>
</div>
  
<script>
  let element = document.getElementById("msg");
  var i = 5;
  var interval = setInterval(function() {
    i--;
    element.innerText = `<%- msg %> (${i})`;
    if(i == 0) {
      clearInterval(interval);
      element.innerHTML = `<a id="msg" href="<%- link %>">Click For Link!</a>`;
    }
  }, 1000)
</script>
  
