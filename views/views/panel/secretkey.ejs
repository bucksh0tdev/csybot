<!DOCTYPE html>
<html lang="en">
  <head>
  <%- header %>
  </head>
  <body>
    <div class="container">
      <aside>
        <div class="top">
          <div class="logo">
            <img src="<%- csybot.config.web.apipath %>/libs/img/logo.png" />
            <h2>CsYBot</h2>
          </div>
          <div class="close" id="close-btn">
            <span class="material-icons-sharp">close</span>
          </div>
        </div>

        <div class="sidebar">
          <a href="<%- base %>/panel">
            <span class="material-icons-sharp">grid_view</span>
            <h3>Dashboard</h3>
          </a>
          <a href="<%- base %>/panel/servers">
            <span class="material-icons-sharp">dns</span>
            <h3>Servers</h3>
          </a>
          <a href="<%- base %>/panel/smart-chatbot" class="active">
            <span class="material-icons-sharp">smart_toy</span>
            <h3>Secret Key</h3>
          </a>
          <a href="<%- base %>/exit">
            <span class="material-icons-sharp">logout</span>
            <h3>Logout</h3>
          </a>
        </div>
      </aside>
      <!------------------ END OF ASIDE ------------------>
      <main>
        <h1 style="font-size: 20px;">Secret Key <span style="font-size: 30px;">smart-chatbot</span></h1>
        <div class="date">
          <input disabled value="<%- csybot.date(); %>" type="date" />
        </div>


        <!------------ END OF INSIGHTS -------------->
        <style>
          .search {
            width: 100%;
            align-items: center;
          }
          

          .areaa {
            display: block;
            width: 50%;
            padding: 0.375rem 0.75rem;
            font-size: 1rem;
            font-weight: 400;
            line-height: 1.5;
            color: #ad91ff;
            background: #202528;
            box-shadow: 0 2rem 3rem rgba(0, 0, 0, 0.4);
            transition: all 300ms ease;
          }    
          
          input[type="submit"] {
            width: 90px;
            background: transparent;
            cursor: pointer;
            padding: 10px 20px;
            border-radius: 4px;
            box-sizing: border-box;
            
            border-radius: 1.2rem;
            background: #202528;
            gap: 1rem;
            box-shadow: 0 2rem 3rem rgba(0, 0, 0, 0.4);
            transition: all 300ms ease;
            color: orange;
          }
          
        </style>
        <center>
        <br>
        <div class="search">          
          
        <form action="<%- base %>/panel/smart-chatbot" method="post">
          
          <% if(secretkey) { %>
            <h2>You Secret Key:</h2>
            
            <input style="height: calc(1.5em + 0.75rem + 2px);" class="areaa" type="text" value="<%- secretkey %>" readonly>
            <br>
            
            <h2>Using Bdfd;</h3>
            <textarea class="areaa" style="resize: none;" rows="10" disabled>$nomention
$botTyping
$try
$httpAddHeader[Authorization;<%- secretkey %>]
$httpAddHeader[Version;<%- csybot.config.dependencies.smart_chatbot.version %>]
$httpAddHeader[using;bdfd]
$httpGet[<%- csybot.config.web.apipath %>/chatbot?user=$authorID&language=tr&botname=CsYBot&ownername=bucksh0t&message=$replaceText[$replaceText[$replaceText[$replaceText[$replaceText[$replaceText[$message;ı;i];ö;o];ğ;g];ü;u];ç;c];ş;s]]
$httpResult[message]
$catch
Üzgünüm cevap veremedim.
$endtry
$argsCheck[>1;Lütfen Mesaj girin]
            </textarea>
            <h3>Select BdScript 2</h3>
            <br>
            <h2>Using Discord.js;</h3>
            <textarea class="areaa" style="resize: none;" rows="10" disabled>const chatbot = require("smart-chatbot"); 
const chatclient = new chatbot.Client({ secretkey: "<%- secretkey %>", botname: "BOT NAME", ownername: "OWNER NAME" });

client.on("message", async(message) => {
  if(!message || !message.content || !message.author || message.author.bot) return;
  
  if (message.content.startsWith(`<@${client.user.id}>`) || message.content.startsWith(`<@!${client.user.id}>`)) {
       let args = message.content.split(" ").slice(1);
       let soru = args.slice(0).join(' ');

       let reply = await chatclient.chat({ 
          message: soru, 
          user: message.author.id,
          language: "tr" // (tr, en)
        });
     message.channel.send(reply);
  }
});</textarea>
            
            
          <% } else { %>
            <br><br>
            <input type="submit" value="Generate">
          <% } %>
          
          
        </form>
          </div>
        </center>
      </main>
      <!----------------- END OF MAIN -------------------->

      <div class="right">
        <div class="top">
          <button id="menu-btn">
            <span class="material-icons-sharp">menu</span>
          </button>
          <div class="profile">
            <div class="info">
              <p>Hey, <b><%- csybot.filter(`${user.username}#${user.discriminator}`); %></b></p>
                <% if(user.id == csybot.config.ownerid) { %>
                  <small class="text-muted" style="cursor: pointer;" onclick="window.location.href='<%- base %>/panel/admin'">Admin</small>
                <% }; %>
            </div>
            <div class="profile-photo">
              <img src="<%- `https://cdn.discordapp.com/avatars/${user.id}/${user.avatar}.png` %>" />
            </div>
          </div>
        </div>
        <!-- END OF TOP -->
        <div class="sales-analytics">
          <h2>Your Features</h2>
        <% if (features) { %>
          <div class="item customers">
            <div class="icon">
              <span class="material-icons-sharp">person</span>
            </div>
            <div class="right">
              <div class="info">
                <h3>SOUND CHANCE</h3>
              </div>
              <h5 class="success">+35%</h5>
            </div>
          </div>
        <% } else { %>   
          <div class="item offline">
            <div class="right">
              <div class="info">
                <h3>it will appear here</h3>
              </div>
            </div>
          </div>
          <% }; %>  
        </div>
      </div>
    </div>
    
  <script src="http://code.jquery.com/jquery-2.0.3.min.js"></script>
  <script src="<%- csybot.config.web.apipath %>/libs/js/panel.js"></script>
  <%- realfooter %>
  </body>
  <%- pwa %>
</html>