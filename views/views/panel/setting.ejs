<!DOCTYPE html>
<html lang="en">
  <head>
  <%- header %>
  </head>
  <body>
    <div class="container">
      <aside>
        <div class="top">
          <div class="logo">
            <img src="<%- csybot.config.web.apipath %>/libs/img/logo.png" />
            <h2>CsYBot</h2>
          </div>
          <div class="close" id="close-btn">
            <span class="material-icons-sharp">close</span>
          </div>
        </div>

        <div class="sidebar">
          <a href="<%- base %>/panel">
            <span class="material-icons-sharp">grid_view</span>
            <h3>Dashboard</h3>
          </a>
          <a href="<%- base %>/panel/servers" class="active">
            <span class="material-icons-sharp">dns</span>
            <h3>Servers</h3>
          </a>
          <a href="<%- base %>/panel/smart-chatbot">
            <span class="material-icons-sharp">smart_toy</span>
            <h3>Secret Key</h3>
          </a>
          <a href="<%- base %>/exit">
            <span class="material-icons-sharp">logout</span>
            <h3>Logout</h3>
          </a>
        </div>
      </aside>
      <!------------------ END OF ASIDE ------------------>
      <main>
        <a href="<%- base %>/panel/server/<%- guild.id %>"><h1><span style="color: yellow;">< </span> <%- csybot.filter(guild.name); %></h1></a>
        <div class="date">
          <input disabled value="<%- csybot.date(); %>" type="date" />
        </div>


        <!------------ END OF INSIGHTS -------------->
        <style>
          .search {
            width: 100%;
            align-items: center;
          }
          

          select {
            display: block;
            width: 280px;
            height: calc(1.5em + 0.75rem + 2px);
            padding: 0.375rem 0.75rem;
            font-size: 1rem;
            font-weight: 400;
            line-height: 1.5;
            color: #ad91ff;
            background: #202528;
            box-shadow: 0 2rem 3rem rgba(0, 0, 0, 0.4);
            transition: all 300ms ease;
          }    
          
          input[type="submit"] {
            width: 90px;
            background: transparent;
            cursor: pointer;
            padding: 10px 20px;
            border-radius: 4px;
            box-sizing: border-box;
            
            border-radius: 1.2rem;
            background: #202528;
            gap: 1rem;
            box-shadow: 0 2rem 3rem rgba(0, 0, 0, 0.4);
            transition: all 300ms ease;
            color: orange;
          }
          
        </style>
        <center>
        <br>
        <div class="search">          
      <h2><%- name %></h2>
          
        <form action="" method="post">
          <select type="option" name="result">
            <option value="open" <%- (select == "open") ? "selected" : "" %>>Open</option>
            <option value="close" <%- (select == "close") ? "selected" : "" %>>Close</option>
          </select>
          <br><br>
          <input type="submit" value="Edit">
        </form>          
          </div>
        </center>
      </main>
      <!----------------- END OF MAIN -------------------->

      <div class="right">
        <div class="top">
          <button id="menu-btn">
            <span class="material-icons-sharp">menu</span>
          </button>
          <div class="profile">
            <div class="info">
              <p>Hey, <b><%- csybot.filter(`${user.username}#${user.discriminator}`); %></b></p>
                <% if(user.id == csybot.config.ownerid) { %>
                  <small class="text-muted" style="cursor: pointer;" onclick="window.location.href='<%- base %>/panel/admin'">Admin</small>
                <% }; %>
            </div>
            <div class="profile-photo">
              <img src="<%- `https://cdn.discordapp.com/avatars/${user.id}/${user.avatar}.png` %>" />
            </div>
          </div>
        </div>
        <!-- END OF TOP -->
        <div class="sales-analytics">
          <h2>Your Features</h2>
        <% if (features) { %>
          <div class="item customers">
            <div class="icon">
              <span class="material-icons-sharp">person</span>
            </div>
            <div class="right">
              <div class="info">
                <h3>SOUND CHANCE</h3>
              </div>
              <h5 class="success">+35%</h5>
            </div>
          </div>
        <% } else { %>   
          <div class="item offline">
            <div class="right">
              <div class="info">
                <h3>it will appear here</h3>
              </div>
            </div>
          </div>
          <% }; %>  
        </div>
      </div>
    </div>
    
  <script src="http://code.jquery.com/jquery-2.0.3.min.js"></script>
  <script src="<%- csybot.config.web.apipath %>/libs/js/panel.js"></script>
  <%- realfooter %>
  </body>
  <%- pwa %>
</html>